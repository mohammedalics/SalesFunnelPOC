<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="EmployeesLovView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  SelectList="Employees.EMP_ID, 
       Employees.EMP_JOB_TITLE, 
       Employees.EMP_NAME"
  FromList="EMPLOYEES Employees"
  OrderBy="Employees.EMP_NAME"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  UseGlueCode="false">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.oracle.anc.fmwpoc.sf.model.views.lov.EmployeesLovView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <EntityUsage
    Name="Employees"
    Entity="com.oracle.anc.fmwpoc.sf.model.entities.Employees"
    ReadOnly="true"
    DeleteParticipant="false"/>
  <ViewAttribute
    Name="EmpId"
    IsUpdateable="false"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="EmpId"
    EntityUsage="Employees"
    AliasName="EMP_ID"/>
  <ViewAttribute
    Name="EmpJobTitle"
    PrecisionRule="true"
    EntityAttrName="EmpJobTitle"
    EntityUsage="Employees"
    AliasName="EMP_JOB_TITLE"/>
  <ViewAttribute
    Name="EmpName"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="EmpName"
    EntityUsage="Employees"
    AliasName="EMP_NAME"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.oracle.anc.fmwpoc.sf.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
