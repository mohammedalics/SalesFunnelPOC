      <from-activity-id id="__6">CreateInsert</from-activity-id>
      <control-flow-case id="__9">
        <from-outcome id="__7">CreateInsert</from-outcome>
        <to-activity-id id="__8">account-edit-btf</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__14">
      <from-activity-id id="__15">account-edit-btf</from-activity-id>
      <control-flow-case id="__17">
        <from-outcome id="__16">accEditCommit</from-outcome>
        <to-activity-id id="__18">accCreateCommit</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__19">
        <from-outcome id="__20">accEditRollback</from-outcome>
        <to-activity-id id="__21">accCreateRollback</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
